//c++

class Solution {
public:
    bool arrayStringsAreEqual(vector<string>& word1, vector<string>& word2) {
        int n=word1.size();
        int m=word2.size();
        // string wo1="";
        // string wo2="";

        for(int i=1;i<n;i++){
            word1[0]+=word1[i];
        }
        for(int i=1;i<m;i++){
            word2[0]+=word2[i];
        }
        if(word1[0]==word2[0]) return true;
        
        return false;

    }
};
